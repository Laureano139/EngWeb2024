extends layout 

block content
    div.w3-card-4
        header.w3-container.w3-teal 
            h1 Compositor: #{compositor.nome}
            a(href="javascript:history.back()")Voltar
        
        div.w3-container 
            ul.w3-ul.w3-card-4(style="width:100%")
                li b Nome:  #{compositor.nome}
                li b Número:  #{compositor.id}
                li b Biografia:  #{compositor.bio}
                li b Data de Nascimento:  #{compositor.dataNasc}
                li b Data de Óbito:  #{compositor.dataObito}
                li b Periodo:
                    a(href='/periodos/' + compositor.periodo.id)= compositor.periodo.nome
            
        
        footer.w3-container.w3-teal
            h5 Generated by A97569::EngWeb2024 in #{data}